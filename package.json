{
	"name": "nestjs-open-source-template",
	"description": "\uD83D\uDCDC My template for creating NestJS Open Source projects",
	"version": "5.5.4",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky install .github/husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore src/**/*.ts"
	},
	"lint-staged": {
		"*.ts": "npm run format"
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "17.6.1",
		"@commitlint/config-angular": "17.6.1",
		"@favware/npm-deprecate": "1.0.7",
		"@nestjs/common": "9.4.0",
		"@nestjs/core": "9.4.0",
		"@types/node": "18.15.11",
		"@typescript-eslint/eslint-plugin": "5.58.0",
		"@typescript-eslint/parser": "5.58.0",
		"eslint": "8.38.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-prettier": "4.2.1",
		"husky": "8.0.3",
		"jest": "29.5.0",
		"lint-staged": "13.2.1",
		"prettier": "2.8.7",
		"reflect-metadata": "0.1.13",
		"release-it": "15.10.1",
		"rimraf": "5.0.0",
		"rxjs": "7.8.0",
		"ts-node": "10.9.1",
		"typescript": "5.0.4"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0",
		"@nestjs/core": "^8.0.0 || ^9.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0"
	},
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.0.0"
	}
}
