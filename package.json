{
	"name": "nestjs-open-source-template",
	"description": "ðŸ“œ My template for creating NestJS Open Source projects",
	"version": "1.0.0",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-pattern .gitignore src/**/*.ts",
		"lint:fix": "npm run lint -- --fix"
	},
	"lint-staged": {
		"*.ts": [
			"npm run format",
			"npm run lint:fix"
		]
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "19.8.0",
		"@commitlint/config-angular": "19.8.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.23.0",
		"@favware/npm-deprecate": "2.0.0",
		"@nestjs/common": "10.4.15",
		"@nestjs/core": "10.4.15",
		"@nestjs/testing": "10.4.15",
		"@release-it/conventional-changelog": "^10.0.0",
		"@types/jest": "29.5.14",
		"@types/node": "22.13.16",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "10.1.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "5.2.5",
		"globals": "^16.0.0",
		"husky": "9.1.7",
		"jest": "29.7.0",
		"lint-staged": "15.5.0",
		"prettier": "3.5.3",
		"reflect-metadata": "0.2.2",
		"release-it": "18.1.2",
		"rimraf": "6.0.1",
		"rxjs": "7.8.2",
		"ts-jest": "29.3.0",
		"ts-node": "10.9.2",
		"typescript": "5.8.2",
		"typescript-eslint": "^8.28.0"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
		"@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.2.0"
	}
}
