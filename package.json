{
	"name": "nestjs-open-source-template",
	"description": "\uD83D\uDCDC My template for creating NestJS Open Source projects",
	"version": "1.0.0",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore src/**/*.ts",
		"lint:fix": "npm run lint -- --fix"
	},
	"lint-staged": {
		"*.ts": ["npm run format", "npm run lint:fix"]
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "19.6.0",
		"@commitlint/config-angular": "19.6.0",
		"@favware/npm-deprecate": "2.0.0",
		"@nestjs/common": "10.4.15",
		"@nestjs/core": "10.4.13",
		"@nestjs/testing": "10.4.15",
		"@types/jest": "29.5.14",
		"@types/node": "22.10.1",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"eslint": "8.57.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.2.1",
		"husky": "9.1.7",
		"jest": "29.7.0",
		"lint-staged": "15.2.10",
		"prettier": "3.4.2",
		"reflect-metadata": "0.2.2",
		"release-it": "17.10.0",
		"rimraf": "6.0.1",
		"rxjs": "7.8.1",
		"ts-jest": "29.2.5",
		"ts-node": "10.9.2",
		"typescript": "5.7.2"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
		"@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.2.0"
	}
}
